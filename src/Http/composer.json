{
  "name": "horizom/http",
  "description": "The Horizom Http package.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://horizom.github.io",
  "keywords": [
    "horizon",
    "horizom",
    "http",
    "response",
    "request",
    "file",
    "php"
  ],
  "support": {
    "issues": "https://github.com/horizom/horizom/issues",
    "source": "https://github.com/horizom/horizom"
  },
  "authors": [
    {
      "name": "Roland Edi",
      "email": "lambirou225@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "horizom/dispatcher": "^3.0",
    "illuminate/support": "^8.0",
    "middlewares/utils": "^3.2",
    "nyholm/psr7": "^1.5",
    "php-http/message-factory": "^1.0",
    "symfony/http-foundation": "^5.4",
    "symfony/mime": "^5.4"
  },
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "Horizom\\Http\\": ""
    }
  },
  "suggest": {
    "horizom/core": "Required to use the Response view method."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}